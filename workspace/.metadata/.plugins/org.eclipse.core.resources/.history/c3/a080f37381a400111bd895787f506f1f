package jipplugin;

import org.eclipse.core.runtime.IStatus;

// the following technique is borrowed from the
// Eclipse Plug-ins book
public class 
ActivatorLog 
{
	public static void
	logMessage
	( String message )
	{
		
	}
	
	public static void
	logError
	( String message, Throwable Exception )
	{
		
	}
	
	private static void 
	logStatus
	(IStatus status)
	{
		Activator.getDefault().getLog().log(status);
	}
}

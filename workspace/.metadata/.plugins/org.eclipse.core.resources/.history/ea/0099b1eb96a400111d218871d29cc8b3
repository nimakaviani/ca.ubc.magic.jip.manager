		= "actions.menu.messages"; //$NON-NLS-1$
	public static String JIP_Label;
	public static String JIP_Tooltip;
	static {
		// initialize resource bundle
		NLS.initializeMessages(BUNDLE_NAME, Messages.class);
	}

	private Messages() {
	}
}

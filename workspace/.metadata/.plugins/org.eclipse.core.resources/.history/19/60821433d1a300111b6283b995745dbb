package jipplugin;

import org.eclipse.core.runtime.PlatformObject;

import com.jchapman.jipsnapman.models.ISnapshotPathModel;

public class
Model 
extends PlatformObject 
implements ISnapshotPathModel
{
	String path = new String("");
	
	@Override
	public String
	getSnapshotPath() 
	{
		return this.path;
	}

	@Override
	public void 
	setSnapshotPath
	(String path) 
	{
		this.path = path;
	}
}

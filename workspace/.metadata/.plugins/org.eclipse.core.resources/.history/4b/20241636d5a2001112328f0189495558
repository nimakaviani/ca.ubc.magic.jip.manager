package actions;

import org.eclipse.jface.action.Action;
import org.eclipse.jface.dialogs.MessageDialog;


public class 
StartAction 
extends Action 
{
	Shell shell;
	
	public 
	StartAction()
	{
		this.shell = new shell();
	}
	@Override
	public void run(){
		MessageDialog.openInformation( this.shell, "First plug-in", "Start!" );
	}
}
